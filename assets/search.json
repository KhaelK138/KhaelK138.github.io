---
layout: null
---
[
  {% assign filtered_pages = "" | split: "" %}
  {% for page in site.pages %}
    {% unless page.hidden %}{% if page.path contains 'markdown/' and page.path contains '.md' and page.layout == 'blank' %}
      {% assign filtered_pages = filtered_pages | push: page %}
    {% endif %}{% endunless %}
  {% endfor %}
  {% for page in filtered_pages %}
    {
      "title": "{{ page.pagetitle | default: page.name | replace: '.md', '' | replace: '-', ' ' | replace: '_', ' ' }}",
      "path": "{{ page.url }}",
      "content": {{ page.content | markdownify | strip_html | normalize_whitespace | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
